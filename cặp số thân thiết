//cặp số thân thiết
#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a, b;
    cin >> a >> b;
    vector<int> pr;

    vector<int> list;

    for (int i = 2; i < 1000; i++)
    {
        int dem = 0;
        for (int j = 2; j <= sqrt(1000); j++)
        {
            if (i % j == 0)
                dem++;
        }
        if (dem == 1)
            pr.push_back(i);
    }
    for (int i = a; i > 1; i--)
    {
        for (int j = a; j >= 0; j--)
        {
            if (a % i == 0 && i == pr[j])
            {
                list.push_back(i);
            }
        }
    }
    for (int i = 2; i <= b; i++)
    {
        for (int j = b; j >= 0; j--)
        {
            if (b % i == 0 && i == pr[j])
            {
                list.push_back(i);
            }
        }
    }
    cout << *list.begin() << " " << *list.rbegin() << "\n";
    if (*list.begin() == *list.rbegin())
        cout << "cap so than thiet";
    else
        cout << "cap so KO than thiet";
    system("pause");
    return 0;
}
