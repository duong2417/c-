#include <iostream>
using namespace std;
void ten(string ten)
{
    cout << ten + " cao nhat";
}
int main()
{
    float a, b, c;
    cin >> a >> b >> c;
    if (a >= b && a > c)
        ten("AN");
    if (b >= c && b > a)
        ten("BINH");
    if (c >= a && c > b)
        ten("CUC");
    system("pause");
    return 0;
}
